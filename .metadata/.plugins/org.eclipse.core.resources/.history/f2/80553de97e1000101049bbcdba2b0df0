package rrrr;

import java.io.IOException;
class Static {
	public int a = 20;
	static int b = 0;
}



public class asa {

	public static void main(String[] args)throws IOException {
		
		int a = 10;
		Static.b = a;
		
		System.out.println(Static.b);
		System.out.println(Static.b++);
		System.out.println(Static.b);
		

		
	}

}
