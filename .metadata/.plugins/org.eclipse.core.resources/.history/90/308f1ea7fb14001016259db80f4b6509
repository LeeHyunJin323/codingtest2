package codingtest;

import java.util.Scanner;

public class Practice1 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int q = sc.nextInt();
		int p = sc.nextInt();
		
		
		int result = 0;
		int count = 0;
		for (int i = 0; i<q; i++) {
			if(q%i == 0)
				count++;
			
			if(count == i)
				result = i;
		}

		

	}

}
