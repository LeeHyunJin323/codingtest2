package codingtest;

import java.util.Scanner;

public class Practice1 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int q = sc.nextInt();
		int p = sc.nextInt();
		
		int result = 0;
		int count = 0;
		
		//약수는 몫이 0일때 그 숫자의 약수
		for (int i = 1; i<=q; i++)
		{
			if(q%i == 0) {
				count++;
			}
			if(count == p) {
				result = i;
				System.out.println(result);
				break;
			}
				
		}



		System.out.println(-1);

	}

}
